E = 0.000001 # max/initial E-field amplitude

timeStep = 0.01 # time step in fs

omega = 0.38 # driving frequency of E-field

RWA = .FALSE. # .TRUE. to utilize RWA, .FALSE. otherwise

itScheme = 1 # iteration scheme, 1=Euler, 2=Verlet, 3=Runge-Kutta 2nd order, 4=Runge-Kutta 4th order

bands = 1 2 # whitespace separated list of band indices for electron transfer

CHGint = 0 # interval to save CHG files. 0=don't save them